{
  "preset": "basic",
  "severity": "warning",
  "rules": {
    "metaTags": {
      "title-exists": true,
      "meta-description-exists": true,
      "viewport-meta-exists": true
    },
    "headings": {
      "h1-exists": true,
      "h1-count-valid": true
    },
    "images": {
      "images-have-alt": true
    },
    "performance": {
      "load-time-acceptable": true
    },
    "security": {
      "https-enabled": {
        "enabled": true,
        "severity": "error"
      }
    }
  }
}
